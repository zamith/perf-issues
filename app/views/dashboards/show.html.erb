<h1>Dashboard for <%= current_user.name %></h1>

<h2>Books by User</h2>
<ul>
  <% @users.each do |user| %>
    <li>
      <h3><%= user.name %></h3>
      <ul>
        <% user.my_books.each do |book| %>
          <li><%= book.name %> by <%= book.author %></li>
        <% end %>
      </ul>
    </li>
  <% end %>
</ul>
